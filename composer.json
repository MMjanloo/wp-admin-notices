{
  "name": "thankstowp/wp-admin-notices-ttwp",
  "description": "Manages WordPress Admin Notices easily",
  "keywords": [
    "wordpress",
    "admin",
    "notice",
    "notices"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Pablo Pacheco",
      "email": "pablo.sg.pacheco@gmail.com"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "type": "wordpress-plugin",
  "require": {
    "php": ">=5.3.2",
    "composer/installers": ">=v1.2.0",
    "tareq1988/wordpress-settings-api-class": "dev-master"
  },
  "extra": {
    "installer-paths": {
      "vendor/{$vendor}/{$name}": [
        "type:wordpress-plugin",
        "type:wordpress-muplugin"
      ]
    }
  },
  "autoload": {
    "classmap": [
      "src/classes/"
    ],
    "files": [
      "src/functions/functions.php"
    ]
  }
}